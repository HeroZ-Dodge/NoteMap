<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:background="@drawable/app_back_group"
              android:orientation="vertical">


    <FrameLayout
        android:id="@+id/frame_layout"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1">


    </FrameLayout>


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/white_10"
        android:orientation="horizontal">


        <TextView
            android:id="@+id/tv_near_by"
            style="@style/text_map_point_list_tab"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:drawableTop="@drawable/ic_near_by"
            android:text="@string/near_by"/>

        <TextView
            android:id="@+id/tv_history"
            style="@style/text_map_point_list_tab"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:drawableTop="@drawable/ic_history"
            android:text="@string/history_map_point_list"/>


        <TextView
            android:id="@+id/tv_setting"
            style="@style/text_map_point_list_tab"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:drawableTop="@drawable/ic_setting"
            android:text="@string/setting"/>


    </LinearLayout>


</LinearLayout>